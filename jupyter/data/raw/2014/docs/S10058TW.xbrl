r:#a9a9a9;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-color:#a9a9a9;border-right-color:#a9a9a9;"&gt;&lt;p class="smt_tblR" style="text-align:left;padding-right:0pt;"&gt; &lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr style="height:15.8pt"&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-color:#a9a9a9;border-right-color:#a9a9a9;"&gt;&lt;p class="smt_tblL" style="text-align:center;padding-left:13.5pt;"&gt;計&lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-color:#a9a9a9;border-right-color:#a9a9a9;"&gt;&lt;p class="smt_tblR" style="padding-right:0pt;letter-spacing:0pt;line-height:10pt;"&gt;1,461&lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-color:#a9a9a9;border-right-color:#a9a9a9;"&gt;&lt;p class="smt_tblR" style="text-align:left;padding-right:0pt;"&gt; &lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-color:#a9a9a9;border-right-color:#a9a9a9;"&gt;&lt;p class="smt_tblR" style="padding-right:0pt;letter-spacing:0pt;line-height:10pt;"&gt;1,441&lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-color:#a9a9a9;border-right-color:#a9a9a9;"&gt;&lt;p class="smt_tblR" style="text-align:left;padding-right:0pt;"&gt; &lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;p style="clear:both; line-height:0.75pt; width:100%; font-size:0.75pt;"&gt; &lt;/p&gt;&lt;/div&gt;&lt;p class="smt_text6"&gt; &lt;/p&gt;&lt;p class="smt_text6"