&lt;tbody&gt;
&lt;tr&gt;
&lt;td style="width: 85px; border-left: 1px solid transparent; border-right: 1px solid transparent"/&gt;
&lt;td style="width: 17px; border-left: 1px solid transparent; border-right: 1px solid transparent"/&gt;
&lt;td style="width: 85px; border-left: 1px solid transparent; border-right: 1px solid transparent"/&gt;
&lt;td style="width: 17px; border-left: 1px solid transparent; border-right: 1px solid transparent"/&gt;
&lt;td style="width: 79px; border-left: 1px solid transparent; border-right: 1px solid transparent"/&gt;
&lt;td style="width: 17px; border-left: 1px solid transparent; border-right: 1px solid transparent"/&gt;
&lt;td style="border-left: 1px solid transparent; border-right: 1px solid transparent"/&gt;
&lt;/tr&gt;
&lt;tr style="min-height: 19px"&gt;
