style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblR" style="padding-right:17.85pt;"&gt;20～40&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr style="height:18.45pt"&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblL"&gt;中型店&lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblL"&gt;101～200㎡&lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblR" style="padding-right:17.85pt;"&gt;2,000&lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblR" style="padding-right:17.85pt;"&gt;2,000&lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblR" style="padding-right:17.85pt;"&gt;40～80&lt;/p&gt;&lt;/td&gt;