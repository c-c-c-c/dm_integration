om:0pt;"&gt;&lt;table cellspacing="0" cellpadding="0" style="border-collapse:collapse;border:solid 0pt #000000;width:429.9pt;"&gt;&lt;colgroup&gt;&lt;col style="width:51pt;min-width:51pt;"/&gt;&lt;col style="width:59.3pt;min-width:59.3pt;"/&gt;&lt;col style="width:71.3pt;min-width:71.3pt;"/&gt;&lt;col style="width:54pt;min-width:54pt;"/&gt;&lt;col style="width:126pt;min-width:126pt;"/&gt;&lt;col style="width:36.8pt;min-width:36.8pt;"/&gt;&lt;col style="width:31.5pt;min-width:31.5pt;"/&gt;&lt;/colgroup&gt;&lt;tr style="height:25.5pt"&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-color:#a9a9a9;border-bottom-color:#a9a9a9;border-left-color:#a9a9a9;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblC" style="line-height:8.5pt;font-size:8.5pt;"&gt; &lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblC" style="line-height:8.5pt;font-size:7.5pt;"&gt;氏名&lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblC" style="line-height:8.5pt;font-size:7.5pt;"&gt;生年月日&lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" colspan="2" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblC" style="line-height:8.5pt;font-size:7.5pt;"&gt;略歴&lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style: