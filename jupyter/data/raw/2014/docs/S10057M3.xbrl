  <jpcrp_cor:CompanyHistoryTextBlock contextRef="FilingDateInstant">&lt;p style="page-break-before:always; line-height:0.75pt; width:100%; font-size:0.75pt;"&gt; &lt;/p&gt;&lt;h3 class="smt_head2"&gt;２ 【沿革】&lt;/h3&gt;&lt;div class="tbld" style="margin-left:1.4pt;margin-right:0pt;margin-top:0pt;margin-bottom:0pt;"&gt;&lt;table cellspacing="0" cellpadding="0" style="border-collapse:collapse;border:solid 0pt #000000;width:467.3pt;"&gt;&lt;colgroup&gt;&lt;col style="width:84.8pt;min-width:84.8pt;"/&gt;&lt;col style="width:382.5pt;min-width:382.5pt;"/&gt;&lt;/colgroup&gt;&lt;tr style="height:18pt"&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblC" style="line-height:18pt;"&gt;年 月&lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblC" style="line-height:18pt;"&gt;概　要&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr style="height:36pt"&gt;&lt;td valign="top" style="vertical-align:top;border-top-style:solid;border-top-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblC" style="text-align:center;padding-left:0pt;padding-right:0pt;margin-top:4pt;margin-bottom:0pt;text-justify:inter-ideograph;text-indent:0pt;font-family:&amp;apos;ＭＳ 明朝&amp;apos;;letter-spacing:0pt;line-height:12pt;"&gt;平成25年 10月&lt;/p&gt;&lt;/td&gt;&lt;td valign="top" style="vertical-align:top;border-top-style:solid;border-top-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblL" s