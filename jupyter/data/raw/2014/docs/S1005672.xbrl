  <jpcrp_cor:RevenuesFromExternalCustomersInformationForEachRegionTextBlock contextRef="Prior1YearDuration">&lt;p class="smt_text6" style="padding-left:9pt;"&gt;(1) 売上高&lt;/p&gt;&lt;div class="tbld" style="margin-left:5pt;"&gt;&lt;table cellspacing="0" cellpadding="0" style="border-collapse:collapse;border:solid 0pt #000000;width:468pt;"&gt;&lt;colgroup&gt;&lt;col style="width:156pt;min-width:156pt;"/&gt;&lt;col style="width:156pt;min-width:156pt;"/&gt;&lt;col style="width:156pt;min-width:156pt;"/&gt;&lt;/colgroup&gt;&lt;tr style="height:10pt"&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-color:#a9a9a9;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-color:#a9a9a9;border-right-color:#a9a9a9;"&gt;&lt;p class="smt_tblC"&gt; &lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-color:#a9a9a9;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-color:#a9a9a9;border-right-color:#a9a9a9;"&gt;&lt;p class="smt_tblC"&gt; &lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-color:#a9a9a9;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-color:#a9a9a9;border-right-color:#a9a9a9;"&gt;&lt;p class="smt_tblR" style="padding-right:9.1pt;"&gt;(単位：千円)&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr style="height:22.5pt"&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblC"&gt;日本&lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblC"&gt;海外&lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:midd