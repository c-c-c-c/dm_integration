  <jpcrp_cor:CompanyHistoryTextBlock contextRef="FilingDateInstant">&lt;p style="page-break-before:always; line-height:0.75pt; width:100%; font-size:0.75pt;"&gt; &lt;/p&gt;&lt;h3 class="smt_head2"&gt;２ 【沿革】&lt;/h3&gt;&lt;p class="smt_unitR"&gt; &lt;/p&gt;&lt;div class="tbld" style="text-align:left;"&gt;&lt;table cellspacing="0" cellpadding="0" style="border-collapse:collapse;border:solid 0pt #000000;width:467.8pt;" class="align_left"&gt;&lt;colgroup&gt;&lt;col style="width:63.3pt;min-width:63.3pt;"/&gt;&lt;col style="width:404.6pt;min-width:404.6pt;"/&gt;&lt;/colgroup&gt;&lt;tr style="height:17pt"&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblC" style="margin-top:1.5pt;margin-bottom:1.5pt;letter-spacing:0pt;"&gt;年月&lt;/p&gt;&lt;/td&gt;&lt;td valign="middle" style="vertical-align:middle;border-top-style:solid;border-top-width:0.75pt;border-bottom-style:solid;border-bottom-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblC" style="margin-top:1.5pt;margin-bottom:1.5pt;letter-spacing:0pt;"&gt;概要&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr style="height:17pt"&gt;&lt;td valign="top" style="vertical-align:top;border-top-style:solid;border-top-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblC" style="text-align:justify;padding-left:1.7pt;margin-top:1.5pt;margin-bottom:1.5pt;letter-spacing:0pt;"&gt;大正12年４月&lt;/p&gt;&lt;/td&gt;&lt;td valign="top" style="vertical-align:top;border-top-style:solid;border-top-width:0.75pt;border-left-style:solid;border-left-width:0.75pt;border-right-style:solid;border-right-width:0.75pt;"&gt;&lt;p class="smt_tblL" style="margin-top:1.5pt;margin-bo